<!-- 用户登录 -->
<!DOCTYPE html>
<html>
<head>
    <% include header.ejs %>
</head>
<body>
<% include navbar.ejs %>
<main class="bs-docs-masthead" id="content" role="main">
    <div class="container">
        <form class="form form-horizontal" role="form" action="/register/" method="post">
            <!-- 注册 -->
            <div class="page-header">
                <p class="lead">用户注册</p>
            </div>
            <div class="form-group">
                <a class="btn btn-info btn-lg" href="https://github.com/login/oauth/authorize?client_id=70d60893e7ec2ee42e40&scope=user:email&redirect_uri=https://elasticbox.com/services/security/github_callback?accept_terms=true">
                    <span class="sso sso-github"></span><span>使用Github账号</span></a>
            </div>

            <div class="form-group">
                <a class="btn btn-primary btn-lg" id="sign-up" href="/own/"><span class="sso sso-email"></span><span>使用个人邮箱</span></a>
            </div>

            <div class="form-group">
                <a class="btn btn-success btn-lg" href="https://accounts.google.com/o/oauth2/auth?client_id=726336950247-p6172q2aojfuntnp8e0rocg45c19ss4b.apps.googleusercontent.com&response_type=code&scope=profile+email&redirect_uri=https%3A//elasticbox.com/services/security/google_callback%3Faccept_terms%3Dtrue&approval_prompt=auto">
                    <span class="sso sso-google"></span><span>使用google账号</span></a>
            </div>
    </form>
    </div>
</main>
</body>
</html>